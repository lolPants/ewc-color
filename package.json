{
  "name": "ewc-color",
  "version": "0.1.0",
  "description": "Convert a Hex string to an ABGR number. Useful for ewc.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "test": "npm run check && npm run lint && npm run ava",
    "ava": "nyc --reporter=html --reporter=text ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "check": "npm run build -- --noEmit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prebuild": "rimraf lib",
    "build": "tsc --sourceMap",
    "watch": "tsc --watch",
    "dev": "npm run watch -- --sourceMap"
  },
  "repository": "git@github.com:lolPants/ewc-color.git",
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "keywords": [
    "ewc",
    "color",
    "hex",
    "number",
    "string",
    "rgba",
    "abgr"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lolPants/ewc-color/issues"
  },
  "homepage": "https://github.com/lolPants/ewc-color#readme",
  "devDependencies": {
    "@types/node": "^11.13.7",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.5"
  }
}
